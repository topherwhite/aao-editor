    <!-- photographer meta -->
    <photographerid>whitec</photographerid>
    <originalfilename>dsc01234.raw</originalfilename>
    <originalformat>RAW</originalformat>
    <originalfilesize>1024</originalfilesize>
    <originalwidth>800</originalwidth>
